<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE sqlMap PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN" "http://ibatis.apache.org/dtd/sql-map-2.dtd">
<sqlMap namespace="PCProvinceSourceDao">

    <select id="update" parameterClass="map">
        <![CDATA[
        SELECT $tableName$
        SET `state` = #auditLog.state#, `username` = #auditLog.username#,
        `source` = #auditLog.source#, `ip` = #auditLog.ip#
        WHERE `id` = #auditLog.id#
        ]]>
        <if test="1=1">
            AND  1=1
        </if>
    </select>

</sqlMap>
